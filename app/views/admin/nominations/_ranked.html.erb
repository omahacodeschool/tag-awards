<table class="collection-data" aria-labelledby="page-title">
  <thead>
    <tr>
      <th class="cell-label" scope="col">Rank</th>
      <th class="cell-label" scope="col">Theater</th>
      <th class="cell-label" scope="col">Show</th>
      <th class="cell-label" scope="col">Nominee</th>
      <th class="cell-label" scope="col">Role</th>
      <th class="cell-label" scope="col">Weighted</th>
      <th class="cell-label" scope="col">Raw</th>

      <!-- <th colspan="2" scope="col"></th> -->
    </tr>
  </thead>

  <tbody>
    <% Nomination.top_ten(award.id, limit).each_with_index do |nomination, i| %>
      <tr class="table__row" tabindex="0">
        <td><%= i + 1 %></td>
        <td><%= nomination.theater %></td>
        <td><%= nomination.show %></td>
        <td><%= nomination.nominee %></td>
        <td><%= nomination.role %></td>
        <td><%= nomination.weighted_count %></td>
        <td><%= nomination.raw_count %></td>
      </tr>
    <% end %>
  </tbody>
</table>